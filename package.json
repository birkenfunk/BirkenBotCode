    "discord.js": "^13.8.1",
    "dotenv": "^16.0.1",
    "glob": "^8.0.3",
    "license-checker": "^25.0.1"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/node": "^18.0.4",
    "eslint": "^8.19.0",
    "ts-node": "^10.8.2",
